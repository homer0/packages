{
  "name": "eslint-plugin-homer0",
  "description": "My custom configurations for ESLint",
  "version": "5.0.0",
  "repository": "homer0/eslint-plugin-homer0",
  "author": "Leonardo Apiwan (@homer0) <me@homer0.com>",
  "license": "MIT",
  "dependencies": {
    "eslint": "^7.4.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsdoc": "^29.1.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-sort-class-members": "^1.7.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "^3.2.0",
    "husky": "^4.2.5",
    "leasot": "^11.1.0",
    "lint-staged": "^10.2.11",
    "semantic-release": "^17.1.1"
  },
  "engine-strict": true,
  "engines": {
    "node": ">=10.0.0"
  },
  "main": "index.js",
  "husky": {
    "hooks": {
      "pre-commit": "./utils/hooks/pre-commit",
      "post-merge": "./utils/hooks/post-merge",
      "prepare-commit-msg": "./utils/hooks/prepare-commit-msg"
    }
  },
  "lint-staged": {
    "*.js": "eslint"
  },
  "scripts": {
    "lint": "./utils/scripts/lint",
    "lint:all": "./utils/scripts/lint-all",
    "todo": "./utils/scripts/todo"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
